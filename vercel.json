{
  "version": 2,
  "builds": [
    {
      "src": "api/generate-poetry.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-poetry",
      "dest": "/api/generate-poetry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
